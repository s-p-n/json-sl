{
    "inputs": {
        "me": {
            "dodge": 20,
            "offense": 20,
            "parry": 20,
            "defense": "dodge+parry"
        },
        "opponent": {
            "dodge": 40,
            "offense": 40,
            "parry": 20,
            "defense": "dodge+parry"
        }
    },
    "f": 0,
    "hit_difference": 0,
    "hit_chance": {
        "me": "parent.inputs.me",
        "opp": "parent.inputs.opponent",
        "hit_difference": "me.offense - opp.defense",
        "hit_difference[84,]": 84,
        "hit_difference[,-84]": -84,
        "f": 0,
        "value": 0,
        "hit_difference[,0]": {
            "value": "parent.hit_difference",
            "parent.f": "1.006*(-value) - 0.006*(-value)^2",
            "parent.value": "50 - parent.f"
        },
        "hit_difference[0,]": {
            "value": "parent.hit_difference",
            "parent.f": "1.006*(value) - 0.006*(value)^2",
            "parent.value": "50 + parent.f"
        },
        "parent.f": "f",
        "parent.hit_difference": "hit_difference"
    },
    "roll": "rand(1,100)",
    "band": 5,
    "min": 8,
    "max": 93,
    "hit_chance[,min + band]": "min + band",
    "does_hit": ".Miss",
    "roll[1, min]": {
        "value": "parent.roll",
        "parent.does_hit": ".Hit"
    },
    "roll[max,100]": {
        "value": "parent.roll",
        "parent.does_hit": ".Miss"
    },
    "roll[min,max]": {
        "value": "parent.roll",
        "opp": "parent.inputs.opponent",
        "me": "parent.inputs.me",
        "chance": "parent.hit_chance",
        "band": "parent.band",
        "does_hit": ".miss",
        "parent.roll[chance - band, chance + band]": {
            "value": "parent.value",
            "parent.does_hit": ".Grazing Blow"
        },
        "parent.roll[,chance - band]": {
            "value": "parent.value",
            "parent.does_hit": ".Hit"
        },
        "parent.roll[chance + band,]": {
            
            "opp": "parent.opp",
            "me": "parent.me",
            "does_hit": "parent.does_hit",
            "parent.does_hit": {
                "opp": "parent.opp",
                "me": "parent.me",
                "roll": "rand(0,opp.defense)",
                "type": "roll",
                "type[,opp.parry]": ".Parry",
                "type[opp.parry,]": ".Dodge",
                "value": {
                    "parry":"parent.opp.parry",
                    "dodge":"parent.opp.dodge",
                    "roll": "parent.roll",
                    "type": "parent.type"
                }
            }
        },
        "parent.does_hit": "does_hit"
    },
    "value": {
        "inputs": "parent.inputs",
        "roll": "parent.roll",
        "hit_chance": "parent.hit_chance",
        "hit_difference": "parent.hit_difference",
        "does_hit": "parent.does_hit"
    }
}